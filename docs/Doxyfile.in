PROJECT_NAME           = "@PROJECT_NAME@"
PROJECT_NUMBER        = "@PROJECT_VERSION@"
STRIP_FROM_PATH       = @CMAKE_SOURCE_DIR@
INPUT                 = @CMAKE_SOURCE_DIR@/src \
                       @CMAKE_SOURCE_DIR@/include
FILE_PATTERNS         = *.cpp *.hpp
RECURSIVE             = YES
OUTPUT_DIRECTORY      = @DOXYGEN_OUTPUT_DIR@
GENERATE_HTML         = YES
GENERATE_LATEX        = NO
GENERATE_XML          = YES
XML_OUTPUT            = xml
EXTRACT_ALL           = YES
EXTRACT_PRIVATE       = YES
EXTRACT_STATIC        = YES